# Deny access to .htaccess itself
<Files .htaccess>
Order allow,deny
Deny from all
</Files>

RewriteEngine On
RewriteBase /wst-ldds/

#RewriteRule ^([^.?]+)$ %{REQUEST_URI}/index.php [L]

RewriteRule ^home?$ index.php [L]
RewriteRule ^signup?$ login/signUp.php [L]
RewriteRule ^signup2?$ login/signUp_2.php [L]
RewriteRule ^event?$ events/index.php [L]
RewriteRule ^events?$ events/index.php [L]
RewriteRule ^events/?$ events/index.php [L]
RewriteRule ^event/(\d+)$ events/event_info.php?eventID=$1 [QSA,L]
RewriteRule ^profile/history/(.+)$ profile/history.php?studentID=$1 [QSA,L]
RewriteRule ^event-data/participant/(\d+)$ event_list/view.php?eventID=$1 [QSA,L]
RewriteRule ^profile/history(/?)$ profile/history.php [L]
RewriteRule ^forgetpassword?$ login/forgetPassword.php [L]
RewriteRule ^forgetpassword2?$ login/forgetPassword2.php [L]
RewriteRule ^forgetpassword3?$ login/forgetPassword3.php [L]
RewriteRule ^forgetpassword4?$ login/forgetPassword4.php [L]

# Disable directory browsing 
Options -Indexes

#Redirect to error page
DirectoryIndex index.php index.html error/index.php
ErrorDocument 403 /wst-ldds/error/
RewriteCond %{REQUEST_FILENAME} !-f  
RewriteCond %{REQUEST_FILENAME} !-d  
RewriteRule .* error [L] 